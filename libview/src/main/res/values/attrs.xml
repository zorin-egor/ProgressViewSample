<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressView">
        <attr name="start_progress" format="integer" />
        <attr name="to_progress" format="integer" />
        <attr name="from_progress" format="integer" />
        <attr name="line_width" format="dimension" />
        <attr name="particle_radius" format="dimension" />
        <attr name="count_down" format="boolean" />
        <attr name="color_random_background" format="boolean" />
        <attr name="dynamic_radius" format="boolean" />
        <attr name="color_background" format="color" />
        <attr name="color_progress" format="color" />
        <attr name="type_progress" format="enum">
            <enum name="one" value="0"/>
            <enum name="two" value="1"/>
            <enum name="three" value="2"/>
            <enum name="four" value="3"/>
            <enum name="five" value="4"/>
        </attr>
    </declare-styleable>
</resources>